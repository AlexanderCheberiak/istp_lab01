@model DormDomain.Model.Student

@{
    ViewData["Title"] = "Імпорт";
}

<div>
    <h3>Оберіть excel-файл для завантаження</h3>
    @using (Html.BeginForm("Import", "Students", FormMethod.Post, new { enctype = "multipart/form-data", id = "frm-excel" }))
    {
        <div>
            Завантажте Excel-файл:
            <input class ="form-control" type="file" name="fileExcel" id="fileExcel" />
            <div>
                <input class="btn btn-success" type="submit" value="Завантажити" />
            </div>
        </div>
    }
</div>
